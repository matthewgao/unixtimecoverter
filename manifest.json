{
  "name": "UnixTime",
  "description": "show readable time of a unixtime stamp",
  "version": "0.1",
  "permissions": ["contextMenus"],
  "background": {
    "scripts": ["sample.js","cal.js"]
  },
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Convert unixtime",
    "default_popup": "popup.html",
    "default_icon" : "icon.png"
  },
  "icons": { "16": "icon.png",
    "48": "icon.png",
   "128": "icon.png" }

}
